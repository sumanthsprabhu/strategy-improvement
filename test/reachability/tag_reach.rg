vars: x1, x2, y1, y2
init: x1 = 0 && y1 = 0 && x2 = 3 && y2 = 3
safe: (x1 < x2 || x2 < x1 || y1 < y2 || y2 < y1)
      && x2 = x2' && y2 = y2'
      && x1 - 1 <= x1' && x1' <= x1 + 1
      && y1 - 1 <= y1' && y1' <= y1 + 1
reach: x1 = x1' && y1 = y1'
       && x2 - 2 <= x2' && x2' <= x2 + 2
       && y2 - 2 <= y2' && y2' <= y2 + 2
