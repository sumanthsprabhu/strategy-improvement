vars: w, x, y, z
init: w = 2 && x = 2 && y = 2 && z = 3
safe:
    (0 <= w' && w' <= w - 1 && x' = x && y' = y && z' = z)
    || (w' = w && 0 <= x' && x' <= x - 1 && y' = y && z' = z)
    || (w' = w && x' = x && 0 <= y' && y' <= y - 1 && z' = z)
    || (w' = w && x' = x && y' = y && 0 <= z' && z' <= z - 1)
reach:
    (0 <= w' && w' <= w - 1 && x' = x && y' = y && z' = z)
    || (w' = w && 0 <= x' && x' <= x - 1 && y' = y && z' = z)
    || (w' = w && x' = x && 0 <= y' && y' <= y - 1 && z' = z)
    || (w' = w && x' = x && y' = y && 0 <= z' && z' <= z - 1)
