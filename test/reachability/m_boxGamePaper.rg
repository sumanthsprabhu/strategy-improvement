vars: x, y, z
init: y = 0
safe: (y <= 1 && -1 <= y) && (z = 0 || z = 1)
      && (z = 0)
      && ((z = 0) && (x' = x) && (y' = y + 1 || y' = y - 1 || y' = y)) || ((z = 1) && (y' = y) && (x' = x + 1 || x' = x - 1 || x' = x))
      && (z' = z - 1)
      
reach: (z = 1)
       && ((z = 0) && (x' = x) && (y' = y + 1 || y' = y - 1 || y' = y)) || ((z = 1) && (y' = y) && (x' = x + 1 || x' = x - 1 || x' = x))
       && (z' = z - 1)
