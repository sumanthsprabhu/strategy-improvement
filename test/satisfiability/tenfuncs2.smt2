(assert (forall ((x Int) (y Int))
		(exists ((f1 Int) (f2 Int) (f3 Int) (f4 Int) (f5 Int) (g1 Int) (g2 Int) (g3 Int) (g4 Int) (g5 Int))
(and (= (+ f1 f1) f2)
 (= (- (+ f1 f2) y) f3)
 (= (+ f2 f2) f4)
 (= (+ f4 f1) f5)
 (= (- f1 y) g1)
 (= (+ 1 g1) g2)
 (= (+ 1 g2) g3)
 (= (+ g3 g3) g4)
 (= (+ g4 f1) g5)))))
