(assert (forall ((x Int))
		(exists ((fives Int) (threes Int))
			(and (<= 0 fives)
			     (<= 0 threes)
			     (=> (<= 15 x)
				 (and (= (+ (* 5 fives) (* 3 threes)) x)
				      (forall ((fivesalt Int) (threesalt Int))
					      (=> (and (<= 0 fivesalt)
						       (<= 0 threesalt)
						       (= (+ (* 5 fivesalt) (* 3 threesalt)) x))
						  (<= (+ threes fives) (+ threesalt fivesalt))))))))))
